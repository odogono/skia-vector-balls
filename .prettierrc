{
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "trailingComma": "none",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^react$",
    "^react-native$",
    "",
    "^@shopify.*",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@.*",
    "^[.]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrderCaseSensitive": true
}